<Page x:Class="WPF_PageNavigation.Pages.Notes"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF_PageNavigation.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Notes">

    <Grid Background="White">
        <Button Content="Back" Command="NavigationCommands.BrowseBack" Margin="10 10 0 0" Padding="5" BorderThickness="0"
                HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content="Notes" VerticalAlignment="Center" HorizontalAlignment="Center"/>
    </Grid>
</Page>
